{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "exactOptionalPropertyTypes": false
  },
  "include": [
    "src"
  ]
}
