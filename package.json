{
  "name": "@searchmcp/client",
  "version": "1.0.3",
  "description": "Thin Node.js client for the SearchMCP REST API",
  "type": "module",
  "homepage": "https://searchmcp.io",
  "repository": {
    "url": "https://github.com/searchmcp/searchmcp-js"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "build:types": "tsc -p tsconfig.build.json",
    "build": "pnpm build:types"
  },
  "exports": {
    ".": {
      "import": "./src/index.js",
      "default": "./src/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
