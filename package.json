{
  "name": "@searchmcp/client",
  "version": "1.0.9",
  "description": "Thin Node.js client for the SearchMCP REST API",
  "type": "module",
  "homepage": "https://searchmcp.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/searchmcp/searchmcp-js.git"
  },
  "scripts": {
    "generate": "rimraf src/generated && openapi-generator-cli generate -c openapi.config.yaml",
    "prepublishOnly": "pnpm run generate && pnpm run build",
    "build": "tsup src/index.ts --format=esm --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "@vitest/coverage-v8": "^3.2.4",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
